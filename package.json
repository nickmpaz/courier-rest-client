{
  "name": "courier-rest-client",
  "publisher": "npaz",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickmpaz/courier-rest-client.git"
  },
  "license": "MIT",
  "displayName": "Courier REST Client",
  "description": "A REST client for VS Code",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:rest-client-library",
    "onView:rest-client-request",
    "onView:rest-client-response"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "courier-rest-client.selectRequest",
        "title": "Select Request"
      },
      {
        "command": "courier-rest-client.pickEnvironment",
        "title": "Pick Environment"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rest-client",
          "title": "Rest Client",
          "icon": "resources/paper-plane.svg"
        }
      ]
    },
    "views": {
      "rest-client": [
        {
          "id": "rest-client-library",
          "type": "tree",
          "name": "Library"
        },
        {
          "id": "rest-client-request",
          "type": "webview",
          "name": "Request"
        },
        {
          "id": "rest-client-response",
          "type": "webview",
          "name": "Response"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.1.0",
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.7",
    "node-fetch": "^2.6.2",
    "vsce": "^2.13.0"
  }
}
